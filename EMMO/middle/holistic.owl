    <owl:ObjectProperty rdf:about="http://emmo.info/emmo/middle/holistic#EMMO_ae2d1a96_bfa1_409a_a7d2_03d69e8a125a">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo/top/mereotopology#EMMO_17e27c22_37e1_468c_9dd7_95e137f73e7f"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo/middle/holistic#EMMO_43e9a05d_98af_41b4_92f6_00f79a09bfce"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo/middle/holistic#EMMO_49804605_c0fe_4538_abda_f70ba1dc8a5d"/>
        <annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">The relation between a process and an object participating to it.</annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <rdfs:comment xml:lang="en">Participation is a parthood relation: you must be part (and then be connected) of the process to contribute to it.</rdfs:comment>
        <rdfs:comment xml:lang="en">Participation is not under direct parthood since a process is not strictly related to reductionism, but it&apos;s a way to categorize temporal regions by the interpreters.</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasParticipant</skos:prefLabel>
    </owl:ObjectProperty>
    



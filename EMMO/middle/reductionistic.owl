    <!-- http://emmo.info/emmo/middle/reductionistic#EMMO_15db234d_ecaf_4715_9838_4b4ec424fb13 -->

    <owl:Class rdf:about="http://emmo.info/emmo/middle/reductionistic#EMMO_15db234d_ecaf_4715_9838_4b4ec424fb13">
        <owl:equivalentClass>
            <owl:Class>
